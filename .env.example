# Home Finder Environment Variables
# Copy to .env and fill in values

# Commute API (choose one)
TRAVELTIME_APP_ID=
TRAVELTIME_API_KEY=
# OR
GOOGLE_MAPS_API_KEY=

# Optional: Google Places for amenities
